{
    "id":"app-specific-vertical-bar",
    "title":"App Specific Vertical Bar",
    "service":"elasticsearch",
    "query":{
        "size":0,
        "query":{
            "bool":{
                "must":[
                    {
                        "range":{
                            "timestamp":{
                                "gte":"now-24h",
                                "lte":"now",
                                "format":"epoch_millis"
                            }
                        }
                    }
                ]
            }
        },
        "aggs":{
            "4":{
                "filters":{
                    "filters":{
                        "Applications":{
                            "query":{
                                "term":{
                                    "Application":"myNTP"
                                }
                            }
                        }
                    }
                },
                "aggs":{
                    "3":{
                        "date_histogram":{
                            "field":"timestamp",
                            "interval":"30s",
                            "time_zone":"America/Los_Angeles",
                            "min_doc_count":1,
                            "extended_bounds":{
                                "min":1473885450891,
                                "max":1473886350891
                            }
                        },
                        "aggs":{
                            "1":{
                                "sum":{
                                    "field":"TotalBytesCount"
                                }
                            },
                            "2":{
                                "sum":{
                                    "field":"TotalPacketsCount"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
