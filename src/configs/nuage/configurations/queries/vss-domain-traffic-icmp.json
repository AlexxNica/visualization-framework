{
    "id":"vss-domain-traffic-icmp",
    "title":"ICMP connections vs Time",
    "service":"elasticsearch",
    "query":{
        "index":"{{index:nuage_flow}}",
        "type":"{{type:nuage_doc_type}}",
        "body":{
            "size":0,
            "query":{
                "bool":{
                    "must":[
                        {
                            "range":{
                                "timestamp":{
                                    "gte":"{{startTime:now-24h}}",
                                    "lte":"{{endTime:now}}",
                                    "format":"epoch_millis"
                                }
                            }
                        }
                    ]
                }
            },
            "aggs": {
                "2": {
                    "filters":{
                        "filters":{
                            "Domain":{
                                "query":{
                                    "term":{
                                        "nuage_metadata.domainName":"{{domainName:chord_domain}}"
                                    }
                                }
                            }
                        }
                    },
                    "aggs": {
                        "4": {
                            "filters": {
                                "filters": {
                                    "ACLALLOW": {
                                        "query": {
                                            "term": {
                                                "type": "ALLOW"
                                            }
                                        }
                                    }
                                }
                            },
                            "aggs": {
                                "5": {
                                    "filters": {
                                        "filters": {
                                            "Protocol": {
                                                "query": {
                                                    "term": {
                                                        "protocol": "ICMP"
                                                    }
                                                }
                                            }
                                        }
                                    },
                                    "aggs": {
                                        "3": {
                                            "date_histogram": {
                                                "field": "timestamp",
                                                "interval": "1h"
                                            },
                                            "aggs": {
                                                "1": {
                                                    "sum": {
                                                        "field": "packets"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
