{
    "id":"wifi-branch-ssid-signal-lastseen",
    "title":"TBD",
    "service":"elasticsearch",
    "query":{
        "index":"{{index:nuage_wifi}}",
        "type":"{{type:nuage_doc_type}}",
        "body":{
            "query":{
                "bool":{
                    "must":[
                        {
                            "range":{
                                "timestamp":{
                                    "gte":"now-1440m",
                                    "lte":"now-1439m",
                                    "format":"epoch_millis"
                                }
                            }
                        },
                        {
                            "term": {
                                "ssid": "{{SSID}}"
                            }
                        }
                    ]
                }
            }
        }
    }
}
